{
    "$schema": "https://www.schemastore.org/package.json",
    "name": "eslint-plugin-typefest",
    "version": "0.1.0",
    "description": "ESLint rules for adopting type-fest and ts-extras conventions.",
    "keywords": [
        "eslint",
        "eslint-plugin",
        "type-fest",
        "ts-extras",
        "typescript"
    ],
    "homepage": "https://github.com/Nick2bad4u/eslint-plugin-typefest",
    "bugs": {
        "url": "https://github.com/Nick2bad4u/eslint-plugin-typefest/issues",
        "email": "20943337+Nick2bad4u@users.noreply.github.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Nick2bad4u/eslint-plugin-typefest.git"
    },
    "license": "MIT",
    "author": "Nick2bad4u <20943337+Nick2bad4u@users.noreply.github.com> (https://nick2bad4u.github.io)",
    "type": "module",
    "main": "./dist/plugin.js",
    "types": "./dist/plugin.d.ts",
    "sideEffects": false,
    "exports": {
        ".": "./dist/plugin.js"
    },
    "files": [
        "dist",
        "docs",
        "README.md",
        "CHANGELOG.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "build:clean": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "pretest": "npm run build",
        "lint:action": "npm run lint:actions",
        "lint:actions": "actionlint -config-file ActionLintConfig.yaml -pyflakes \"\" -shellcheck \"\"",
        "test": "vitest run",
        "lint:prettier": "prettier . --log-level warn --cache --cache-location=.cache/.prettier-cache --cache-strategy=content --check",
        "lint:prettier:fix": "prettier . --log-level warn --cache --cache-location=.cache/.prettier-cache --cache-strategy=content --write",
        "test:watch": "vitest",
        "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.build.json --noEmit && tsc -p tsconfig.eslint.json --noEmit && tsc -p tsconfig.js.json --noEmit",
        "release:check": "npm run lint && npm run typecheck && npm run test && npm pack --dry-run",
        "prepublishOnly": "npm run release:check"
    },
    "dependencies": {
        "@typescript-eslint/utils": "^8.55.0"
    },
    "peerDependencies": {
        "eslint": "^9.0.0"
    },
    "devDependencies": {
        "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
        "@eslint/compat": "^2.0.2",
        "@eslint/js": "^9.39.2",
        "@microsoft/eslint-plugin-sdl": "^1.1.0",
        "@softonus/prettier-plugin-duplicate-remover": "^1.1.2",
        "@types/node": "^25.2.3",
        "@typescript-eslint/parser": "^8.55.0",
        "@typescript-eslint/rule-tester": "^8.55.0",
        "eslint": "^9.39.2",
        "eslint-plugin-canonical": "^5.1.3",
        "eslint-plugin-comment-length": "^2.2.2",
        "eslint-plugin-eslint-plugin": "^7.3.1",
        "eslint-plugin-etc": "^2.0.3",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-jsdoc": "^62.5.5",
        "eslint-plugin-listeners": "^1.5.1",
        "eslint-plugin-math": "^0.13.1",
        "eslint-plugin-module-interop": "^0.3.1",
        "eslint-plugin-n": "^17.24.0",
        "eslint-plugin-no-function-declare-after-return": "^1.1.0",
        "eslint-plugin-no-lookahead-lookbehind-regexp": "^0.4.0",
        "eslint-plugin-no-use-extend-native": "^0.7.2",
        "eslint-plugin-perfectionist": "^5.5.0",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-redos": "^4.5.0",
        "eslint-plugin-regexp": "^3.0.0",
        "eslint-plugin-require-jsdoc": "^1.0.4",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-sonarjs": "^3.0.7",
        "eslint-plugin-sort-class-members": "^1.21.0",
        "eslint-plugin-total-functions": "^7.1.0",
        "eslint-plugin-unicorn": "^63.0.0",
        "eslint-plugin-xss": "^0.1.12",
        "eslint-plugin-zod": "^3.1.0",
        "globals": "^17.3.0",
        "prettier-plugin-ini": "^1.3.0",
        "prettier-plugin-interpolated-html-tags": "^2.0.1",
        "prettier-plugin-jsdoc": "^1.8.0",
        "prettier-plugin-jsdoc-type": "^0.2.0",
        "prettier-plugin-merge": "^0.9.1",
        "prettier-plugin-multiline-arrays": "^4.1.4",
        "prettier-plugin-packagejson": "^3.0.0",
        "prettier-plugin-properties": "^0.3.1",
        "prettier-plugin-sort-json": "^4.2.0",
        "prettier-plugin-toml": "^2.0.6",
        "ts-extras": "^0.17.0",
        "type-fest": "^5.4.4",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.55.0",
        "vitest": "^4.0.18"
    },
    "engines": {
        "node": ">=20.19.0"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
