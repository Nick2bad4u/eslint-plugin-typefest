{
    "$schema": "https://www.schemastore.org/package.json",
    "name": "eslint-plugin-typefest",
    "version": "0.1.0",
    "description": "ESLint rules for adopting type-fest and ts-extras conventions.",
    "keywords": [
        "eslint",
        "eslint-plugin",
        "type-fest",
        "ts-extras",
        "typescript"
    ],
    "homepage": "https://github.com/Nick2bad4u/eslint-plugin-typefest",
    "bugs": {
        "url": "https://github.com/Nick2bad4u/eslint-plugin-typefest/issues",
        "email": "20943337+Nick2bad4u@users.noreply.github.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Nick2bad4u/eslint-plugin-typefest.git"
    },
    "license": "MIT",
    "author": "Nick2bad4u <20943337+Nick2bad4u@users.noreply.github.com> (https://nick2bad4u.github.io)",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": "./dist/plugin.js"
    },
    "main": "./dist/plugin.js",
    "types": "./dist/plugin.d.ts",
    "files": [
        "dist",
        "docs",
        "CHANGELOG.md"
    ],
    "scripts": {
        "bench": "vitest bench --run",
        "bench:compare": "vitest bench --run --compare",
        "bench:ui": "vitest bench --ui",
        "bench:watch": "vitest bench",
        "build": "tsc -p tsconfig.build.json",
        "build:clean": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
        "clean:cache": "rimraf dist coverage cache .cache .vite .turbo",
        "clean:cache:coverage": "rimraf coverage .coverage",
        "clean:cache:dist": "rimraf dist release",
        "clean:cache:eslint": "rimraf .cache/.eslintcache",
        "clean:cache:ncu": "rimraf .cache/.ncu-cache.json",
        "clean:cache:prettier": "rimraf .cache/.prettier-cache .prettier-cache .prettiercache",
        "clean:cache:stryker": "rimraf .stryker-tmp",
        "clean:cache:stylelint": "rimraf .cache/stylelintcache stylelintcache .stylelintcache",
        "clean:cache:temp": "rimraf temp .temp",
        "clean:cache:typescript": "rimraf .cache/**.tsbuildinfo .cache/builds",
        "clean:cache:vite": "rimraf .cache/vite .cache/vitest .cache/vitest-zero-coverage .cache/vite-zero-coverage",
        "clean:database": "rimraf %appdata%/uptime-watcher/uptime-watcher.sqlite",
        "clean:docs": "rimraf docs/docusaurus/docs/** docs/docusaurus/build/** docs/docusaurus/static/eslint-inspector/** docs/docusaurus/static/storybook/** docs/docusaurus/.docusaurus/**",
        "clean:docusaurus": "npm run clean:docs && cd docs/docusaurus && npx docusaurus clear",
        "cognitive-complexity": "cognitive-complexity-ts --threshold 10",
        "commit": "git-cz",
        "contrib": "all-contributors",
        "contrib:add": "all-contributors add",
        "contrib:check": "all-contributors check",
        "contrib:compare": "npm run contrib:check",
        "contrib:generate": "all-contributors generate",
        "coverage": "vitest run --coverage",
        "lint": "cross-env NODE_OPTIONS=--max_old_space_size=16384 eslint --cache --cache-strategy content --cache-location .cache/.eslintcache && echo \"Eslint done!\"",
        "lint:action": "npm run lint:actions",
        "lint:actions": "actionlint -config-file ActionLintConfig.yaml -pyflakes \"\" -shellcheck \"\"",
        "lint:all:fix": "npm run lint:fix && npm run lint:prettier:fix && npm run lint:remark",
        "lint:all:fix:quiet": "npm run lint:fix:quiet && npm run lint:prettier:fix && npm run lint:remark",
        "lint:fix": "cross-env NODE_OPTIONS=--max_old_space_size=16384 eslint --cache --cache-strategy content --cache-location .cache/.eslintcache --fix && echo \"Eslint fix done!\"",
        "lint:fix:quiet": "cross-env UW_ESLINT_PROGRESS=off NODE_OPTIONS=--max_old_space_size=16384 eslint --cache --cache-strategy content --cache-location .cache/.eslintcache --fix && echo \"Eslint fix done!\"",
        "lint:prettier": "prettier . --log-level warn --cache --cache-location=.cache/.prettier-cache --cache-strategy=content --check",
        "lint:prettier:fix": "prettier . --log-level warn --cache --cache-location=.cache/.prettier-cache --cache-strategy=content --write",
        "lint:quiet": "cross-env UW_ESLINT_PROGRESS=nofile NODE_OPTIONS=--max_old_space_size=16384 eslint --cache --cache-strategy content --cache-location .cache/.eslintcache && echo \"Eslint done!\"",
        "lint:remark": "remark --rc-path .remarkrc.mjs --silently-ignore --ignore-path .remarkignore --frail \"*.{md,mdx}\" \"docs/**/*.{md,mdx}\" --quiet",
        "lint:remark:fix": "prettier --log-level warn --ignore-path config/tools/prettierignore.remark --cache --cache-location=.cache/.prettier-cache --cache-strategy=content --no-error-on-unmatched-pattern --write \"*.{md,mdx}\" \"docs/**/*.{md,mdx}\" && npm run remark:fix",
        "lint:yaml": "cross-env NODE_OPTIONS=--max_old_space_size=16384 eslint --cache --cache-strategy content --cache-location .cache/.eslintcache \"**/*.{yml,yaml}\" && echo \"YAML lint done!\"",
        "lint:yaml:fix": "cross-env NODE_OPTIONS=--max_old_space_size=16384 eslint --cache --cache-strategy content --cache-location .cache/.eslintcache --fix \"**/*.{yml,yaml}\" && echo \"YAML lint (fix) done!\"",
        "madge:circular": "madge --circular --no-spinner --ts-config tsconfig.json --extensions ts,tsx,js,jsx,mjs,cjs,cts,mts ./ --exclude \"(^|[\\/])(test|dist|node_modules)($|[\\/])|\\.css$\"",
        "madge:leaves": "madge --leaves --no-spinner --ts-config tsconfig.json --extensions ts,tsx,js,jsx,mjs,cjs,cts,mts ./ --exclude \"(^|[\\/])(test|dist|node_modules)($|[\\/])|\\.css$\"",
        "madge:orphans": "madge --orphans --no-spinner --ts-config tsconfig.json --extensions ts,tsx,js,jsx,mjs,cjs,cts,mts ./ --exclude \"(^|[\\/])(test|dist|node_modules)($|[\\/])|\\.css$\"",
        "open:coverage": "open-cli coverage/index.html",
        "prepublishOnly": "npm run release:check",
        "release:check": "npm run lint && npm run typecheck && npm run test && npm pack --dry-run",
        "remark:fix": "remark --rc-path .remarkrc.mjs --silently-ignore --ignore-path .remarkignore --frail --quiet --output -- \"*.{md,mdx}\" \"docs/**/*.{md,mdx}\"",
        "remark:test-config": "remark --rc-path .remarkrc.mjs --silently-ignore --ignore-path .remarkignore --frail \"README.md\"",
        "pretest": "npm run build",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage --reporter=default",
        "test:coverage:detailed": "vitest run --coverage --reporter=verbose",
        "test:coverage:minimal": "vitest run --coverage --reporter=dot",
        "test:coverage:open": "npm run test:coverage && npm run open:coverage",
        "test:coverage:quiet": "vitest run --coverage --reporter=default --silent",
        "test:coverage:verbose": "vitest run --coverage --reporter=verbose",
        "test:detailed": "vitest run --reporter=verbose",
        "test:minimal": "vitest run --reporter=dot",
        "test:open": "npm run test && npm run open:coverage",
        "test:parallel": "cross-env VITEST_FILE_PARALLELISM=true vitest run",
        "test:quiet": "vitest run --reporter=default --silent",
        "test:serial": "cross-env MAX_THREADS=1 VITEST_FILE_PARALLELISM=false vitest run",
        "test:verbose": "vitest run --reporter=verbose",
        "test:watch": "vitest",
        "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.build.json --noEmit && tsc -p tsconfig.eslint.json --noEmit && tsc -p tsconfig.js.json --noEmit",
        "typecheck:all": "npm run typecheck",
        "types:update": "typesync",
        "update-deps": "npx ncu -i --install never && npm install --force"
    },
